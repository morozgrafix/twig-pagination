{
    "name": "bertramakers/twig-pagination",
    "description": "Twig Pagination Extension",
    "type": "library",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Bert Ramakers",
            "homepage": "http://www.bertramakers.com",
            "role": "Lead Developer"
        }
    ],
    "require": {
      "php": ">=5.5",
      "twig/twig": "^2.4"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.8",
        "phpunit/php-token-stream": "1.4.6",
        "squizlabs/php_codesniffer": "~2.3",
        "phing/phing": "~2.11",
        "satooshi/php-coveralls": "~0.7"
    },
    "autoload": {
        "psr-4": {
            "DevotedCode\\Twig\\Pagination\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DevotedCode\\Twig\\Pagination\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "1.1.x-dev"
        }
    }
}
